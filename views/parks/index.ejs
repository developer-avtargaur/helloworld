<h3>Parks</h3>
  <div class="row">
    <div class= 'col s12'>
<% parks.forEach((park) => { %>


      <div class="col s12 m4">
        <div class="card small">
          <a href='parks/<%= park._id %>' ><div class="card-image">
            <img src="<%= park.photo %>">
          </div>
          <div class= 'center-align'>
            <h4><%= park.parkname %></h4></a>
            </div>
        </div>
      </div>



    <%   }) %>

  </div>
</div>




    <p>Don't see a London park you love? <a href='mailto:parklife@parklife.io'>Suggest it to us!</a></p>
